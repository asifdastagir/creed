        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.handle }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.title }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.products_count</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.products_count }}</span></div>
      {% elsif page %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.id }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.handle }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.title }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.author</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.author }}</span></div>
      {% elsif article %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.id }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.handle }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.title }}</span></div>
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.author</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.author }}</span></div>
