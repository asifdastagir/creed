      {% for blog in blogs %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ blog.handle }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ blog.title }} ({{ blog.articles_count }} articles)</span></div>
      {% endfor %}
    </div>

    <div class="ai-data-panel-{{ ai_gen_id }}" data-panel="metafields">
      <div class="ai-section-title-{{ ai_gen_id }}">Metafields</div>
      {% if page %}
        {% for metafield in page.metafields %}
          {% for namespace in metafield %}
            {% for field in namespace[1] %}
              <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.{{ namespace[0] }}.{{ field[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ field[1].value | truncate: 200 }}</span></div>
            {% endfor %}
          {% endfor %}
        {% endfor %}
      {% elsif product %}
        {% for metafield in product.metafields %}
          {% for namespace in metafield %}
            {% for field in namespace[1] %}
              <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.{{ namespace[0] }}.{{ field[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ field[1].value | truncate: 200 }}</span></div>
            {% endfor %}
          {% endfor %}
        {% endfor %}
      {% elsif collection %}
        {% for metafield in collection.metafields %}
          {% for namespace in metafield %}
            {% for field in namespace[1] %}
              <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.{{ namespace[0] }}.{{ field[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ field[1].value | truncate: 200 }}</span></div>
            {% endfor %}
          {% endfor %}
        {% endfor %}
      {% else %}
        <div class="ai-empty-state-{{ ai_gen_id }}">No metafields available for this page type</div>
      {% endif %}
    </div>

    <div class="ai-data-panel-{{ ai_gen_id }}" data-panel="settings">
