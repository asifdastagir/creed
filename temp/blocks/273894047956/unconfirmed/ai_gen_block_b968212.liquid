      {% for setting in settings %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ setting[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ setting[1] | truncate: 100 }}</span></div>
      {% endfor %}
    </div>
  </div>
</dynamic-data-viewer-{{ ai_gen_id }}><script>
(function() {
  class DynamicDataViewer{{ ai_gen_id }} extends HTMLElement {
    constructor() {
      super();
    }

    connectedCallback() {
      this.tabs = this.querySelectorAll('.ai-data-tab-{{ ai_gen_id }}');
      this.panels = this.querySelectorAll('.ai-data-panel-{{ ai_gen_id }}');
      this.setupEventListeners();
    }

    setupEventListeners() {
      this.tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          const targetPanel = tab.dataset.tab;
          this.switchTab(targetPanel);
        });
      });
    }

    switchTab(targetPanel) {
      this.tabs.forEach(tab => {
        tab.classList.remove('active');if (tab.dataset.tab === targetPanel) {
          tab.classList.add('active');
        }
      });

      this.panels.forEach(panel => {
        panel.classList.remove('active');
        if (panel.dataset.panel === targetPanel) {
          panel.classList.add('active');
        }
      });
    }
  }

  customElements.define('dynamic-data-viewer-{{ ai_gen_id }}', DynamicDataViewer{{ ai_gen_id }});
})();
</script>

