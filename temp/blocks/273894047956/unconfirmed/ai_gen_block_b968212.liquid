{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-dynamic-data-{{ ai_gen_id }} {
    font-family: monospace;
    font-size: {{ block.settings.font_size }}px;
    line-height: 1.4;
    color: {{ block.settings.text_color }};
    background-color: {{ block.settings.background_color }};
    border-radius: {{ block.settings.border_radius }}px;
    border: 1px solid {{ block.settings.border_color }};
    overflow: hidden;
  }

  .ai-data-tabs-{{ ai_gen_id }} {
    display: flex;
    flex-wrap: wrap;
    border-bottom: 1px solid {{ block.settings.border_color }};
    background-color: {{ block.settings.tab_background_color }};
  }

  .ai-data-tab-{{ ai_gen_id }} {
    padding: 12px 16px;
    cursor: pointer;
    border: none;
    background: none;
    color: {{ block.settings.tab_text_color }};
    font-size: {{ block.settings.font_size | minus: 1 }}px;
    border-bottom: 2px solid transparent;
    transition: all 0.3s ease;
    white-space: nowrap;
  }

  .ai-data-tab-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.tab_hover_color }};
  }

  .ai-data-tab-{{ ai_gen_id }}.active {
    color: {{ block.settings.tab_active_text_color }};
    border-bottom-color: {{ block.settings.tab_active_border_color }};
    background-color: {{ block.settings.tab_active_background_color }};
  }

  .ai-data-content-{{ ai_gen_id }} {
    padding: {{ block.settings.padding }}px;
    white-space: pre-wrap;
    word-break: break-word;
    overflow-x: auto;
    max-height: {{ block.settings.max_height }}px;
    overflow-y: auto;
  }

  .ai-data-panel-{{ ai_gen_id }} {
    display: none;}

  .ai-data-panel-{{ ai_gen_id }}.active {
    display: block;
  }

  .ai-data-item-{{ ai_gen_id }} {
    margin-bottom: {{ block.settings.item_spacing }}px;
    padding-left: 15px;
  }

  .ai-data-key-{{ ai_gen_id }} {
    color: {{ block.settings.key_color }};
    font-weight: bold;
  }

  .ai-data-value-{{ ai_gen_id }} {
    color: {{ block.settings.value_color }};
  }

  .ai-data-separator-{{ ai_gen_id }} {
    color: {{ block.settings.separator_color }};
  }

  .ai-section-title-{{ ai_gen_id }} {
    color: {{ block.settings.section_title_color }};
    font-weight: bold;
    font-size: {{ block.settings.font_size | plus: 2 }}px;
    margin-bottom: 10px;
    text-transform: uppercase;
  }

  .ai-empty-state-{{ ai_gen_id }} {
    color: {{ block.settings.separator_color }};
    font-style: italic;
    text-align: center;
    padding: 40px 20px;
  }

  @media screen and (max-width: 768px) {
    .ai-data-tabs-{{ ai_gen_id }} {
      overflow-x: auto;
      flex-wrap: nowrap;
    }
    .ai-data-tab-{{ ai_gen_id }} {
      flex-shrink: 0;
    }
  }
{% endstyle %}

