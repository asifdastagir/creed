{% doc %}
  @prompt
    make the component i want get data from page source, i want just render data only no other thing, where is page source i need to get dynamic data

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-dynamic-data-{{ ai_gen_id }} {
    font-family: monospace;
    font-size: {{ block.settings.font_size }}px;
    line-height: 1.4;
    color: {{ block.settings.text_color }};
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.padding }}px;
    border-radius: {{ block.settings.border_radius }}px;
    white-space: pre-wrap;
    word-break: break-word;
    overflow-x: auto;
    border: 1px solid {{ block.settings.border_color }};
  }

  .ai-data-section-{{ ai_gen_id }} {
    margin-bottom: {{ block.settings.section_spacing }}px;
    padding-bottom: {{ block.settings.section_spacing }}px;
    border-bottom: 1px solid {{ block.settings.border_color }};
  }

  .ai-data-section-{{ ai_gen_id }}:last-child {
    border-bottom: none;
    margin-bottom: 0;padding-bottom: 0;
  }

  .ai-section-title-{{ ai_gen_id }} {
    color: {{ block.settings.section_title_color }};
    font-weight: bold;
    font-size: {{ block.settings.font_size | plus: 2 }}px;
    margin-bottom: 10px;
    text-transform: uppercase;
  }

  .ai-data-item-{{ ai_gen_id }} {
    margin-bottom: {{ block.settings.item_spacing }}px;
    padding-left: 15px;
  }

  .ai-data-key-{{ ai_gen_id }} {
    color: {{ block.settings.key_color }};
    font-weight: bold;}

  .ai-data-value-{{ ai_gen_id }} {
    color: {{ block.settings.value_color }};
  }

  .ai-data-separator-{{ ai_gen_id }} {
    color: {{ block.settings.separator_color }};
  }
{% endstyle %}

<div class="ai-dynamic-data-{{ ai_gen_id }}" {{ block.shopify_attributes }}>

{% if block.settings.show_global_objects %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Global Objects</div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">template</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ template }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page_title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page_title }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">canonical_url</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ canonical_url }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">current_page</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ current_page }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">current_tags</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ current_tags | join: ', ' }}</span></div>
</div>
{% endif %}

{% if block.settings.show_request_data %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Request Data</div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">request.host</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ request.host }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">request.path</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ request.path }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">request.page_type</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ request.page_type }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">request.locale</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ request.locale.iso_code }}</span></div></div>
{% endif %}

{% if block.settings.show_settings_data %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Theme Settings</div>
  {% for setting in settings %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ setting[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ setting[1] | truncate: 100 }}</span></div>
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_all_collections %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">All Collections</div>
  {% for collection in collections %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ collection.handle }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.title }} ({{ collection.products_count }} products)</span></div>
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_all_pages %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">All Pages</div>
  {% for page in pages %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ page.handle }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.title }}</span></div>
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_all_blogs %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">All Blogs</div>
  {% for blog in blogs %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ blog.handle }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ blog.title }}</span></div>
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_metafields and page %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Page Metafields</div>
  {% for metafield in page.metafields %}
    {% for namespace in metafield %}
      {% for field in namespace[1] %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ namespace[0] }}.{{ field[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ field[1].value | truncate: 200 }}</span></div>
      {% endfor %}
    {% endfor %}
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_metafields and product %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Product Metafields</div>
  {% for metafield in product.metafields %}
    {% for namespace in metafield %}
      {% for field in namespace[1] %}
        <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">{{ namespace[0] }}.{{ field[0] }}</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ field[1].value | truncate: 200 }}</span></div>
      {% endfor %}
    {% endfor %}
  {% endfor %}
</div>
{% endif %}

{% if block.settings.show_context_data %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Context Objects</div>
  {% if product %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.id }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.handle }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.title }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.type</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.type }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.vendor</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.vendor }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">product.tags</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ product.tags | join: ', ' }}</span></div>
  {% elsif collection %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.id }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.handle }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.title }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">collection.products_count</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ collection.products_count }}</span></div>
  {% elsif page %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.id }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.handle }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.title }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">page.author</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ page.author }}</span></div>
  {% elsif article %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.id }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.handle</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.handle }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.title</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.title }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">article.author</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ article.author }}</span></div>
  {% endif %}
</div>
{% endif %}

{% if block.settings.show_shop_data %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Shop Data</div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">shop.name</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ shop.name }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">shop.domain</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ shop.domain }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">shop.currency</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ shop.currency }}</span></div>
  <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">shop.money_format</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ shop.money_format }}</span></div>
</div>
{% endif %}

{% if block.settings.show_customer_data %}
<div class="ai-data-section-{{ ai_gen_id }}">
  <div class="ai-section-title-{{ ai_gen_id }}">Customer Data</div>
  {% if customer %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer.id</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ customer.id }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer.email</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ customer.email }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer.first_name</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ customer.first_name }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer.last_name</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ customer.last_name }}</span></div>
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer.orders_count</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">{{ customer.orders_count }}</span></div>
  {% else %}
    <div class="ai-data-item-{{ ai_gen_id }}"><span class="ai-data-key-{{ ai_gen_id }}">customer</span><span class="ai-data-separator-{{ ai_gen_id }}">: </span><span class="ai-data-value-{{ ai_gen_id }}">null (not logged in)</span></div>
  {% endif %}
</div>
{% endif %}

</div>

