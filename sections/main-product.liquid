{% comment %}theme-check-disable TemplateLength, NestedSnippet, UnusedAssign, DeprecateBgsizes{% endcomment %}
{%- liquid
  if "ar he he-IL hi" contains localization.language.iso_code
    assign rtl = '-rtl'
  endif

  assign store_width = settings.container_max_width

  if section.settings.section_width == 'custom'
    assign store_width = section.settings.container_max_width
  endif

  if template.suffix contains 'store-width-'
    assign store_width = template.suffix | split: 'store-width-' | last | plus: 0
  endif

  if section.settings.section_width == 'custom'
    assign style_inline = '--container-max-width:' | append: store_width | append: 'px;'
  endif

  assign aspect_ratio = settings.product_image_aspect_ratio

  if aspect_ratio == 'custom'
    assign aspect_ratio = settings.product_height_ratio | times: 100 | divided_by: settings.product_width_ratio
  endif

  assign in_collection = product.collections | where: 'handle', collection.handle | first
  if collection == null or collection.handle == 'all' or in_collection == nil
  assign all_products_count = 0
  for c in product.collections
  if c.handle != 'all' and c.handle != 'frontpage' and all_products_count < c.all_products_count
  assign collection = c
  assign all_products_count = c.all_products_count
  endif
  endfor
  endif
-%}
{{ 'main-product' | append: rtl | append: '.css' | asset_url | stylesheet_tag }}

{%- liquid
  assign layout = 'product'
  assign section_id = section.id
  assign product_form_id = 'product-form-' | append: section_id | append: '-' | append: product.id
  assign current_variant = product.selected_or_first_available_variant | default: product.variants.first
  assign thumbs = product.metafields.custom.thumbs | default: section.settings.thumbs, allow_false: true
  assign media_layout_grid = false

  if section.settings.media_layout != 'thumbs'
    assign thumbs = false
    assign media_layout_grid = true
  endif

  assign hide_variants = product.metafields.custom.hide_variants
  if hide_variants.value != true
    assign hide_variants = section.settings.hide_variants
  endif

  assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'

  assign media_count = product.media.size
  if hide_variants and media_count > 1 and variant_images.size > 0
    assign media_count = media_count | minus: variant_images.size | plus: 0
  endif

  assign initialSlide = 0
  if product.selected_variant.featured_media
    assign selected_variant_image_id = product.selected_variant.featured_media.id

    for media in product.media
      if selected_variant_image_id == media.id
        assign initialSlide = forloop.index0
      endif
    endfor
  endif

  assign model_variants_schema = product.metafields.custom.model_variants_schema

  if product.metafields.custom.model_with_variants.value == true and model_variants_schema.value != blank
    assign model_variant_name_array = model_variants_schema | split: product.selected_variant.id
    if model_variant_name_array.size > 1
      assign model_variant_name = model_variants_schema | split: product.selected_variant.id | last | split: "]" | first | strip | replace: ',', '' | replace: '"', ''
      assign media_model_viewer_first = product.media | where: 'media_type', 'model' | first
      assign media_model_viewer_first_id = media_model_viewer_first.id

      for media in product.media
        if media_model_viewer_first_id == media.id
          assign initialSlide = forloop.index0
        endif
      endfor
    endif
  endif

  assign options_with_values = product.options_with_values

  assign color_options_name = settings.product_card_color_options_name | split: ","

  assign has_color_option = false

  for option in options_with_values
    assign has_color_option = false

    if color_options_name contains option.name
      assign has_color_option = true
      assign color_option_order = forloop.index0

      break;
    endif
  endfor

  if has_color_option == true
    assign colorGroups = ''

    if product.images.size > 0
      for image in product.images
        unless image.alt contains 'color-'
          continue
        endunless

        assign imageAlt = image.alt | escape | replace: 'color-', '' | handle
        assign colorGroups = colorGroups | append: imageAlt

        unless forloop.last
          assign colorGroups = colorGroups | append: ','
        endunless
      endfor

      assign colorGroupsArray = colorGroups | split: ',' | uniq
      assign colorGroups = colorGroupsArray | join: ','
    endif

    assign optionHandle = current_variant.options[color_option_order] | handle

    for color in colorGroupsArray
      if color == optionHandle
        assign colorSelected = optionHandle

        break
      endif
    endfor
  endif

  if section.settings.bottom_tabs or section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal
    if section.settings.apps_placement == 'tab'
      assign blocks = section.blocks | where: 'type', 'collapsible_tab_apps'
      assign blocks = section.blocks | where: 'type', 'collapsible_tab' | concat: blocks
      assign blocks = section.blocks | where: 'type', 'page_source' | concat: blocks
      assign blocks = section.blocks | where: 'type', 'dynamic_accordion' | concat: blocks
    else
      assign blocks = section.blocks | where: 'type', 'collapsible_tab'
      assign blocks = section.blocks | where: 'type', 'page_source' | concat: blocks
      assign blocks = section.blocks | where: 'type', 'dynamic_accordion' | concat: blocks
    endif

    assign blocks = section.blocks | where: 'type', 'description' | concat: blocks
  endif

  assign hide_quantity = 1

  if settings.product_inventory_quantity and settings.product_inventory_quantity_on_product_page
    assign hide_quantity = 0
  endif
-%}
<style>
  :root section#shopify-section-{{ section.id }} {

    {%- if section.settings.color_title != blank and section.settings.color_title != 'rgba(0,0,0,0)' -%}
      --product-page-title-color: {{ section.settings.color_title }};
    {%- endif -%}
    {%- if section.settings.color_price_regular != blank and section.settings.color_price_regular != 'rgba(0,0,0,0)' -%}
      --product-page-price-actual-color: {{ section.settings.color_price_regular }};
    {%- endif -%}
    {%- if section.settings.color_price_old != blank and section.settings.color_price_old != 'rgba(0,0,0,0)' -%}
      --product-page-price-old-color: {{ section.settings.color_price_old }};
    {%- endif -%}
  }
</style>
<div class="holder mt-global" {% render 'section-style', section: section %} data-section-render>
  <div id="addToCartTopScroll" class="invisible"></div>
  <div class="container show-on-scroll">
    <div class="prd-block row g-4 {% if media_count == 0 or section.blocks.size == 0 %}justify-content-center{% endif %}">

      {%- unless media_count == 0 -%}
        <div class="col-12 col-lg-{% if section.settings.thumbs_position != 'bottom' %}6{% else %}5{% endif %} aside--sticky js-sticky-collision">
          <div class="aside-content">
            <product-gallery {% if media_layout_grid %}data-grid="true" data-media-count="{{ media_count }}"{% endif %} {% if colorSelected != blank %}data-filter="color-{{ colorSelected }}"{% endif %} {% if colorGroups != blank %}data-color-groups="{{ colorGroups }}"{% endif %} class="{% if media_layout_grid %}product-grid-gallery layout-{{ section.settings.media_layout }} {% endif %}d-flex w-100 prd-block-gallery-container{% if section.settings.thumbs_position == 'bottom' %}-hor{% endif %}" data-scroll-sync="{% if hide_variants %}false{% else %}true{% endif %}" data-initial-slide="{{ initialSlide }}" data-aspect="{{ aspect_ratio }}" style="--aspect-ratio: {{ aspect_ratio }}">
              {%- liquid
                if thumbs == true and media_count != 1
                  unless media_count == 1
                    if section.settings.thumbs_position == 'bottom'
                      assign sizes ='(min-width: 1200px) 77px, (min-width: 992px) 55px, (min-width: 768px) 119px, (min-width: 576px) 83px, calc((100vw - 30px - 45px)/4)'
                    else
                      assign sizes ='(min-width: 992px) 72px, (min-width: 768px) 128px, (min-width: 576px) 92px, calc((100vw - 30px - 45px)/4)'
                    endif
                    render 'product-media-thumbs' sizes: sizes, product: product, section: section, product_media_main_fill_color: section.settings.product_media_main_fill_color, media_count: media_count, variant_images: variant_images, hide_variants: hide_variants, thumbs_position: section.settings.thumbs_position, grid: media_layout_grid
                  endunless
                endif

                if section.settings.thumbs_position == 'bottom'
                  assign sizes ='(min-width: 1200px) 559px, (min-width: 992px) 450px, (min-width: 768px) 690px, (min-width: 576px) 510px, (min-width: 360px) 330px, calc(100vw - 30px)'
                else
                  assign sizes ='(min-width: 1200px) 457px, (min-width: 992px) 348px, (min-width: 768px) 690px, (min-width: 576px) 510px, (min-width: 360px) 330px, calc(100vw - 30px)'
                endif
              -%}
              {% render 'product-media-main',
                product: product,
                sizes: sizes,
                product_form_id: product_form_id,
                loop: section.settings.enable_video_looping,
                product_media_main_fill_color: section.settings.product_media_main_fill_color,
                media_count: media_count,
                model_variant_name: model_variant_name,
                grid: media_layout_grid,
                colorSelected: colorSelected
              %}
            </product-gallery>
            {% render 'under-gallery-blocks', section: section, class: 'hide-mobile' %}
          </div>
        </div>
      {%- endunless -%}
      {%- if section.blocks.size > 0 -%}
        <div class="prd-block-info prd-block-info--style2{% if media_layout_grid %} is-sticky{% endif %} col-12 col-lg-{% if section.settings.thumbs_position != 'bottom' %}6{% else %}7{% endif -%}">
        {%- liquid assign tab_apps = section.blocks | where: 'type', 'collapsible_tab_apps' -%}
        {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when '@app' -%}
            <div class="prd-block-row mt-product-global">
              {%- liquid
                if section.settings.apps_placement == 'default' or tab_apps.size == 0
                  render block
                endif
              -%}
            </div>
          {%- when 'divider' -%}
            <div class="prd-info-hr mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}></div>
          {%- when 'html' -%}
            {%- liquid
              assign block_html_metafield = block.settings.html_metafield
              assign block_html_show = true

              if block.settings.show_block_if_html_metafield and block_html_metafield == blank
                assign block_html_show = false
              endif

              assign html = block.settings.html | replace: '&lt;', '<' | replace: '&gt;', '>' | replace: '&amp;', '&' | replace: '&quot;', '"'
            -%}
            {%- if block_html_show -%}
              {%- if block.settings.collapsible_tab -%}
                <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                  <div class="tab-accordion-item">
                    <input type="checkbox" name="tab-accordion" id="tabAccordion{{ block.id }}" {% if block.settings.opened %}checked{% endif %}>
                    <label for="tabAccordion{{ block.id }}" class="text-uppercase">{{ block.settings.heading | default: 'Heading' }}<span class="toggle-arrow"></span></label>
                    <div class='tab-accordion-item-content js-set-height'>
                      <div class="tab-accordion-item-content-inside">
                        <div class="rte">
                          {{ html }}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              {%- else -%}
                <div class="prd-block-row mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                  {{ html }}
                </div>
              {%- endif -%}
            {%- endif -%}

          {%- when 'icons' -%}
            <div class="prd-block-banners mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              <div class="row gx-2 gx-xl-3">
                {%- for i in (1..4) -%}
                  {%- liquid
                    assign icon_enable = 'icon_show_' | append: i
                    assign icon_text = 'icon_title_' | append: i
                    assign icon_code = 'icon_' | append: i
                    assign icon_custom_svg = 'icon_custom_svg_' | append: i
                    assign icon_custom_image = 'icon_custom_image_' | append: i
                    assign only_image = 'only_image_' | append: i
                  -%}
                  {%- if block.settings[icon_enable] -%}
                    <div class="col">
                      {%- if block.settings[only_image] and block.settings[icon_custom_image] != blank -%}
                        {% render 'image-container',
                          image: block.settings[icon_custom_image],
                          sizes: '132px'
                          class_ic: 'w-100',
                          hover_scale: true,
                          lazyload: false,
                          cover: false,
                          bg: false,
                          round: 0,
                          hover_scale: false
                        %}
                      {%- else -%}
                        <div class="prd-block-banner bg-alt">
                          <div class="prd-block-banner-icon">
                            {%- if block.settings[icon_custom_image] != blank -%}
                                {% render 'image-container',
                                  image: block.settings[icon_custom_image],
                                  padding: 100,
                                  sizes: '100px',
                                  class_img: 'w-100',
                                  hover_scale: true,
                                  lazyload: false,
                                  cover: false,
                                  bg: false,
                                  round: 0,
                                  hover_scale: false
                                %}
                            {%- elsif block.settings[icon_custom_svg] != blank -%}
                              {{ block.settings[icon_custom_svg] }}
                            {%- else -%}
                              {% render 'icon' icon: block.settings[icon_code] %}
                            {%- endif -%}
                          </div>
                          <div class="prd-block-banner-text">{{ block.settings[icon_text] }}</div>
                          {% render 'icon', icon: 'border' %}
                        </div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- endfor -%}
              </div>
            </div>
          {%- when 'image' -%}
            {%- if block.settings.payment_enable -%}
              <div class="prd-block-safety  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                <h2 class="h4-style line-text">{{ 'theme.general.guaranteed_safe_checkout' | t }}</h2>
                {%- if block.settings.payment_image != blank -%}
                  {% render 'image-container',
                    image: block.settings.payment_image,
                    sizes: '500px',
                    cover: false,
                    bg: false,
                    round: 0,
                    hover_scale: false
                  %}
                {%- else -%}
                  {% render 'payments', class: 'row g-1 flex-wrap justify-content-center' %}
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- when 'meta' -%}
            <div class="prd-block-meta row  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- if block.settings.rating -%}
                <div class="col-auto">
                  <div class="review-rating">
                    {% render 'rating' product: product, type: 'product' %}
                  </div>
                </div>
              {%- endif -%}

              {%- if block.settings.vendor -%}
                <div class="col-auto">
                  <span>{{ 'theme.general.vendor' | t }}:</span> <a href="{{ product.vendor | url_for_vendor }}">{{ product.vendor }}</a>
                </div>
              {%- endif -%}

              {%- if block.settings.availability -%}
                <div class="col-auto" data-render="avaialbility-status-{{ product_form_id }}">
                  {% render 'product-meta-availability', product: product, section: section, current_variant: current_variant %}
                </div>
              {%- endif -%}
            </div>
          {%- when 'title' -%}
            <h1 class="prd-block-name  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {{ product.title | escape }}
            </h1>
          {%- when 'meta_extended' -%}
            <div class="prd-block-desc  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- if block.settings.title -%}
                <h4 class="prd-block-desc-title">{{ 'theme.general.details' | t }}</h4>
              {%- endif -%}
              <p style="line-height: 1.8667em; {% if block.settings.title %}margin:0;{% endif %}"  data-render="meta-extended-{{ product_form_id }}">
                {%- if block.settings.barcode -%}<b>{{ 'theme.general.barcode' | t }}:</b> {{ product.selected_or_first_available_variant.barcode | default: '-' }}<br>{%- endif -%}
                {%- if block.settings.type and product.type != blank -%}<b>{{ 'theme.general.product_type' | t }}:</b> {{ product.type | link_to_type }}<br>{%- endif -%}
                {%- if block.settings.sku and product.selected_or_first_available_variant.sku != blank -%}<b>{{ 'customer.order.sku' | t }}:</b> {{ product.selected_or_first_available_variant.sku | default: '-' }}<br>{%- endif -%}
                {%- if block.settings.tags and product.tags.size > 0 -%}<b>{{ 'theme.general.tags' | t }}:</b>
                  {% for tag in product.tags %}
                    {%- liquid
                      if tag contains 'badge:'
                        continue
                      endif
                    -%}
                    <a href="{{ routes.all_products_collection_url }}/{{ tag | handle }}">{{ tag }}</a> {% unless forloop.last %},{% endunless %}
                  {% endfor %}
                  <br>
                {%- endif -%}
                {%- liquid assign weight = product.selected_or_first_available_variant.weight -%}
                {%- if block.settings.weight and weight > 0 -%}<b>{{ 'theme.general.weight' | t }}:</b> {{ weight | weight_with_unit }}<br>{%- endif -%}
                {%- if block.settings.vendor -%} <b>{{ 'theme.general.vendor' | t }}:</b> <a href="{{ product.vendor | url_for_vendor }}">{{ product.vendor }}</a> {%- endif -%}
              </p>
            </div>
          {%- when 'wish_free_ship' -%}
            {%- unless settings.catalog_mode -%}
              {%- if block.settings.wishlist or block.settings.free_shipping -%}
                <div class="prd-block-row mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                  <div class="row gx-0">
                    {%- if block.settings.wishlist -%}
                      <div class="col prd-block-links" data-render="wishlist-{{ product_form_id }}">{% render 'wishlist-component-toggle-button', product: product, current_variant: current_variant %}</div>
                    {%- endif -%}
                    {%- if block.settings.free_shipping -%}
                      <div class="col-auto js-contents-free-shipping">{% render 'free-shipping-status' layout: 'product' %}</div>
                    {%- endif -%}
                  </div>
                </div>
              {%- endif -%}
            {%- endunless -%}
          {%- when 'price' -%}
            <div class="prd-block-price no-js-hidden  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- render 'price', render: true, product: product, use_variant: true, price_class: 'price--large', product_form_id: product_form_id, layout: layout -%}
              <div data-render="subscription-{{ product_form_id }}">
              {%- if current_variant.selling_plan_allocations.size > 0 -%}
                {%- liquid
                  assign min = current_variant.selling_plan_allocations[0].per_delivery_price
                  for sa in current_variant.selling_plan_allocations
                    if forloop.first
                      continue
                    endif

                    if sa.per_delivery_price < min
                      assign min = sa.per_delivery_price
                    endif
                  endfor
                  assign difference = current_variant.price | minus: min
                  assign save = difference | times: 100.0 | divided_by: current_variant.price | round
                -%}
                <div class="subscription-label">{{ 'theme.general.badge_cart_subscription' | t }} &bull; {{ 'theme.general.save_up_to' | t }} {% if save < 100 and save > 0 %}{{ save }}%{% endif %}</div>
              {%- endif -%}
              </div>
            </div>
            {%- if block.settings.product_page_price_tax_included and shop.taxes_included -%}
              <div class="prd-block-tax">
                {%- if shop.taxes_included -%}
                  {{ 'products.product.include_taxes' | t }}
                {%- endif -%}
              </div>
            {%- endif -%}
            {%- form 'product', product, id: 'product-form-installment', class: 'installment caption-large' -%}
              <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
              {{ form | payment_terms }}
            {%- endform -%}
          {%- when 'short_description' -%}
            {%- if block.settings.text != blank -%}
              <div class="prd-block-desc  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                {%- if block.settings.show_title -%}
                  <h3 class="prd-block-desc-title d-none d-md-block">{{ 'theme.general.short_description' | t }}</h3>
                {%- endif -%}
                {{ block.settings.text | replace: '&lt;', '<' | replace: '&gt;', '>' | replace: '&amp;', '&' | replace: '&quot;', '"' }}
              </div>
            {%- endif -%}
          {%- when 'variant_picker' -%}
            <div class="mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- liquid
                assign swatches_width = settings.product_swatches_page_color_min_width

                if settings.product_swatches_page_inherits_global
                  assign swatches_width = settings.product_swatches_color_min_width
                endif
              -%}
              {%- render 'variant_picker',
                product: product,
                picker_type: block.settings.picker_type,
                shopify_attributes: block.shopify_attributes,
                section: section,
                section_id: section_id,
                layout: layout,
                product_form_id: product_form_id,
                use_pics: block.settings.use_pics,
                use_variants_media: block.settings.use_variants_media,
                color_options_name: settings.product_card_color_options_name,
                swatches_width: swatches_width
              -%}
              {%- liquid assign buy_buttons_block_size = section.blocks | where: 'type', 'buy_buttons' | size -%}
              {%- if buy_buttons_block_size == 0 -%}
                {%- form 'product', product, id: product_form_id, class: 'form js-product-form', novalidate: 'novalidate' -%}
                  <input type="hidden" name="id" value="{{ current_variant.id }}">
                {%- endform -%}
              {%- endif -%}
            </div>
          {%- when 'fbt' -%}
            <div class="mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- liquid
                if block.settings.style == 'list'
                  assign style = 'inner-list'
                elsif block.settings.style == 'grid' or block.settings.style == 'grid2'
                  assign style = 'inner-grid'
                endif

                assign section_fbt_id = 'frequently-bought-together-view-' | append: style

                if settings.fbt_source == 'collection'
                  assign data_ajax = product.url | append: '?section_id=' | append: section_fbt_id
                else
                  assign data_ajax = routes.product_recommendations_url | append: '?section_id=' | append: section_fbt_id | append: '&product_id=' | append: product.id | append: '&limit=10&intent=complementary'
                endif
              -%}
              {%- if block.settings.placement == 'inner' -%}
                <frequently-bought-together class="lazyload mt-product-global {% if request.design_mode %}ajax-loaded{% endif %}" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }} {% unless request.design_mode %}data-ajax="{{ data_ajax }}" data-expand="200"{% endunless %}>
                  {%- if request.design_mode -%}
                    {% render 'frequently-bought-together-controller' layout: style %}
                  {%- endif -%}
                </frequently-bought-together>
              {%- endif -%}
            </div>
          {%- when 'buy_buttons' -%}
            <style>
              :root section#shopify-section-{{ section.id }} {
                --button-addtocart-desktop-height: {{ block.settings.button_height }}px;
                --button-addtocart-mobile-height: {{ block.settings.button_height_mobile }}px;
              }
            </style>
            <div class=" mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {% render 'product-buy-buttons'
                block: block,
                product: product,
                product_form_id: product_form_id,
                section_id: section_id,
                layout: layout,
                show_dynamic_checkout: block.settings.show_dynamic_checkout,
                show_cart_note: block.settings.show_cart_note,
                hide_quantity: hide_quantity,
                hide_add_to_cart: block.settings.button_add_to_cart_hide
              %}
            </div>
          {%- when 'text' -%}
            <p class="mt-product-global product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"  {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {{- block.settings.text -}}
            </p>
          {%- when 'description' -%}
            {%- if section.settings.bottom_tabs == false and product.description != blank and section.settings.inner_tabs_to_horizontal == false -%}
              <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                <div class="tab-accordion-item">
                  <input type="checkbox" name="tab-accordion" id="tabAccordion{{ block.id }}" {% if block.settings.opened %}checked{% endif %}>
                  <label for="tabAccordion{{ block.id }}" class="text-uppercase">{{ block.settings.heading | default: block.settings.page.title }}<span class="toggle-arrow"></span></label>
                  <div class='tab-accordion-item-content js-set-height'>
                    <div class="tab-accordion-item-content-inside">
                      <div class="rte">
                        {{ product.description }}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {%- endif -%}
          {%- when 'custom_liquid' -%}
            <div class=" mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {{ block.settings.custom_liquid }}
            </div>
          {%- when 'collapsible_tab' -%}
            {%- if section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal == false -%}
              <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                <div class="tab-accordion-item">
                  <input type="checkbox" name="tab-accordion" id="tabAccordion{{ block.id }}" {% if block.settings.opened %}checked{% endif %}>
                  <label for="tabAccordion{{ block.id }}" class="text-uppercase">{{ block.settings.heading | default: block.settings.page.title }}<span class="toggle-arrow"></span></label>
                  <div class='tab-accordion-item-content js-set-height'>
                    <div class="tab-accordion-item-content-inside">
                      <div class="rte">
                        {{ block.settings.content | replace: '&lt;', '<' | replace: '&gt;', '>' | replace: '&amp;', '&' | replace: '&quot;', '"' }}
                        {{ block.settings.page.content }}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {%- endif -%}
          {%- when 'collapsible_tab_apps' -%}
            {%- if section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal == false -%}
              <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                <div class="tab-accordion-item">
                  <input type="checkbox" name="tab-accordion" id="tabAccordion{{ tab_apps.first.id }}" {% if tab_apps.first.settings.opened %}checked{% endif %}>
                  <label for="tabAccordion{{ tab_apps.first.id }}" class="text-uppercase">{{ tab_apps.first.settings.heading }}<span class="toggle-arrow"></span></label>
                  {%- if section.settings.apps_placement == 'tab' -%}
                    <div class='tab-accordion-item-content js-set-height'>
                      <div class="tab-accordion-item-content-inside">
                        {%- liquid
                          assign apps = section.blocks | where: 'type', '@app'
                          for app in apps
                            render app
                          endfor
                        -%}
                      </div>
                    </div>
                  {%- endif -%}
                </div>
              </div>
            {%- endif -%}
          {%- when 'page_source' -%}
            {%- if section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal == false -%}
              <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                <div class="tab-accordion-item">
                  <input type="checkbox" name="tab-accordion" id="tabAccordion{{ block.id }}" {% if block.settings.opened %}checked{% endif %}>
                  <label for="tabAccordion{{ block.id }}" class="text-uppercase">{{ block.settings.heading | default: 'View Page Source' }}<span class="toggle-arrow"></span></label>
                  <div class='tab-accordion-item-content js-set-height'>
                    <div class="tab-accordion-item-content-inside">
                      <div class="rte">
                        <pre style="background: #f5f5f5; padding: 15px; border-radius: 5px; overflow-x: auto; font-size: 12px; line-height: 1.4; white-space: pre-wrap; word-wrap: break-word;"><code>{{ content_for_layout | escape }}</code></pre>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {%- endif -%}
          {%- when 'dynamic_accordion' -%}
            {%- if section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal == false -%}
              <div class="block-accordion  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
                {%- liquid
                  assign accordion_items = ''
                  assign item_count = 0
                  
                  # Collect product description
                  if block.settings.show_product_description and product.description != blank
                    assign item_count = item_count | plus: 1
                    assign accordion_items = accordion_items | append: 'product_description|' | append: item_count | append: '|'
                  endif
                  
                  # Collect meta fields
                  if block.settings.show_meta_fields
                    for meta_field in product.metafields
                      if meta_field.value != blank and meta_field.value != null
                        assign item_count = item_count | plus: 1
                        assign accordion_items = accordion_items | append: 'meta_field|' | append: item_count | append: '|' | append: meta_field.key | append: '|' | append: meta_field.namespace | append: '|'
                      endif
                    endfor
                  endif
                  
                  # Collect page data
                  if block.settings.show_page_data and block.settings.page != blank
                    assign item_count = item_count | plus: 1
                    assign accordion_items = accordion_items | append: 'page_data|' | append: item_count | append: '|'
                  endif
                  
                  # Collect custom content
                  if block.settings.show_custom_content and block.settings.custom_content != blank
                    assign item_count = item_count | plus: 1
                    assign accordion_items = accordion_items | append: 'custom_content|' | append: item_count | append: '|'
                  endif
                  
                  # Collect product specifications
                  if block.settings.show_specifications
                    for spec in product.metafields.specifications
                      if spec.value != blank and spec.value != null
                        assign item_count = item_count | plus: 1
                        assign accordion_items = accordion_items | append: 'specification|' | append: item_count | append: '|' | append: spec.key | append: '|'
                      endif
                    endfor
                  endif
                  
                  # Collect product features
                  if block.settings.show_features
                    for feature in product.metafields.features
                      if feature.value != blank and feature.value != null
                        assign item_count = item_count | plus: 1
                        assign accordion_items = accordion_items | append: 'feature|' | append: item_count | append: '|' | append: feature.key | append: '|'
                      endif
                    endfor
                  endif
                  
                  assign items_array = accordion_items | split: '|'
                  assign current_item = 0
                -%}
                
                {%- for item in items_array -%}
                  {%- if item != blank -%}
                    {%- assign item_parts = item | split: '|' -%}
                    {%- assign item_type = item_parts[0] -%}
                    {%- assign item_id = item_parts[1] -%}
                    {%- assign current_item = current_item | plus: 1 -%}
                    
                    <div class="tab-accordion-item">
                      <input type="checkbox" name="tab-accordion" id="tabAccordion{{ block.id }}_{{ current_item }}" {% if block.settings.opened or current_item == 1 %}checked{% endif %}>
                      <label for="tabAccordion{{ block.id }}_{{ current_item }}" class="text-uppercase">
                        {%- case item_type -%}
                          {%- when 'product_description' -%}
                            {{ block.settings.product_description_heading | default: 'Product Description' }}
                          {%- when 'meta_field' -%}
                            {{ item_parts[2] | replace: '_', ' ' | capitalize }}
                          {%- when 'page_data' -%}
                            {{ block.settings.page_data_heading | default: block.settings.page.title | default: 'Page Information' }}
                          {%- when 'custom_content' -%}
                            {{ block.settings.custom_content_heading | default: 'Additional Information' }}
                          {%- when 'specification' -%}
                            {{ item_parts[2] | replace: '_', ' ' | capitalize }}
                          {%- when 'feature' -%}
                            {{ item_parts[2] | replace: '_', ' ' | capitalize }}
                        {%- endcase -%}
                        <span class="toggle-arrow"></span>
                      </label>
                      <div class='tab-accordion-item-content js-set-height'>
                        <div class="tab-accordion-item-content-inside">
                          <div class="rte">
                            {%- case item_type -%}
                              {%- when 'product_description' -%}
                                {{ product.description }}
                              {%- when 'meta_field' -%}
                                {%- assign meta_key = item_parts[2] -%}
                                {%- assign meta_namespace = item_parts[3] -%}
                                {%- assign meta_value = product.metafields[meta_namespace][meta_key] -%}
                                {%- if meta_value.value -%}
                                  {{ meta_value.value }}
                                {%- else -%}
                                  {{ meta_value }}
                                {%- endif -%}
                              {%- when 'page_data' -%}
                                {{ block.settings.page.content }}
                              {%- when 'custom_content' -%}
                                {{ block.settings.custom_content | replace: '&lt;', '<' | replace: '&gt;', '>' | replace: '&amp;', '&' | replace: '&quot;', '"' }}
                              {%- when 'specification' -%}
                                {%- assign spec_key = item_parts[2] -%}
                                {%- assign spec_value = product.metafields.specifications[spec_key] -%}
                                {%- if spec_value.value -%}
                                  {{ spec_value.value }}
                                {%- else -%}
                                  {{ spec_value }}
                                {%- endif -%}
                              {%- when 'feature' -%}
                                {%- assign feature_key = item_parts[2] -%}
                                {%- assign feature_value = product.metafields.features[feature_key] -%}
                                {%- if feature_value.value -%}
                                  {{ feature_value.value }}
                                {%- else -%}
                                  {{ feature_value }}
                                {%- endif -%}
                            {%- endcase -%}
                          </div>
                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                {%- endfor -%}
              </div>
            {%- endif -%}
          {%- when 'ask_about_the_product' -%}
            <div class="prd-block-links  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {%- liquid
                assign text = 'theme.general.ask_about_this_product' | t
                if block.settings.text != blank
                  assign text = block.settings.text
                endif
              -%}
              <info-popup data-subtitle="{{ 'theme.general.have_a_question_subtitle' | t }}" data-selector="#modalInfo{{ block.id }}"><a href="#" class="modal-info-link" aria-controls="modalInfo{{ block.id }}" aria-haspopup="dialog"><span class="icon-wrap">{% render 'icon-mail3' %}</span> <span>{{ text }}</span></a></info-popup>
              <div class="modal-wrap modal-info-content" style="display: none; width: 570px;" id="modalInfo{{ block.id }}" role="dialog" aria-label="{{ block.settings.text }}" aria-modal="true" tabindex="-1"> {% render 'ask-about-the-product', current_variant: current_variant %}</div>
            </div>
          {%- when 'popup' -%}
            <div class="prd-block-links  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              <info-popup data-subtitle="{{ block.settings.subtitle }}" data-ajax="{{ block.settings.page.url }}?section_id=async-info-popup" data-selector="#modalInfo{{ block.id }}"><a href="#" class="modal-info-link" aria-controls="modalInfo{{ block.id }}" aria-haspopup="dialog"><span class="icon-wrap">{% render 'icon', icon: block.settings.icon %} </span> <span>{{ block.settings.text | default: block.settings.page.title }}</span></a></info-popup>
              <div class="modal-wrap modal-info-content" style="display: none; width: 970px;" id="modalInfo{{ block.id }}" role="dialog" aria-label="{{ block.settings.text }}" aria-modal="true" tabindex="-1"> </div>
            </div>
          {%- when 'share' -%}
            <div class="text-start mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
            {% render 'share', section: section, block: block %}
            </div>
          {%- when 'rating' -%}
            <div class="review-rating  mt-product-global" {% render 'product-block-style', block: block %} {{ block.shopify_attributes }}>
              {% render 'rating' product: product, type: 'product' %}
            </div>
          {%- endcase -%}
        {%- endfor -%}

        {%- if section.settings.bottom_tabs == false and section.settings.inner_tabs_to_horizontal -%}
          {%- if blocks.size > 0 or tab_apps.size > 0 -%}
            <div class="mt-product-global" style="--margin-top-product-blocks:70px;">
              {% render 'product-tabs-core', blocks: blocks, product: product, section: section %}
            </div>
          {%- endif -%}
        {%- endif -%}

        {%- form 'contact', class: 'contact-form' -%}
          {%- if form.posted_successfully? -%}
            <div tabindex="-1" autofocus>
              {%- liquid
                assign message = 'templates.contact.form.post_success' | t
                render 'success', message: message, large: 1
              -%}
            </div>
          {%- elsif form.errors -%}
            <div tabindex="-1" autofocus>
              {%- liquid
                assign message = 'templates.contact.form.error_heading' | t
                render 'error', message: message
              -%}
            </div>
          {%- endif -%}
        {%- endform -%}
        {% render 'under-gallery-blocks', section: section, class: 'show-mobile' %}
      </div>
      {%- endif -%}
    </div>
</div>
</div>

