{%- comment -%}theme-check-disable NestedSnippet,DeprecateBgsizes,TemplateLength, UnclosedHTMLElement{%- endcomment -%}

{%- liquid
  assign per_row = section.settings.per_row | plus: 0
  assign per_row_mobile = section.settings.per_row_mobile | plus: 0
  assign slideshow_control_layout = section.settings.slideshow_control_layout
  assign layout = section.settings.layout

  assign sizes = '(min-width: 1400px) 1147px, (min-width: 992px) 992px, (min-width: 768px) 768px, calc(100vw - 30px)'

  if section.settings.fluid
    if per_row == 1
      assign sizes = '(min-width: 1400px) 1920px, (min-width: 992px) 992px, (min-width: 768px) 768px, calc(100vw - 30px)'
    else
      assign sizes = '(min-width: 1400px) 864px, (min-width: 992px) 720px, (min-width: 768px) 566px, calc(100vw - 30px)'
    endif
  endif

  assign round = 0
  assign inner_space = ' g-0'
  if section.settings.inner_space
    assign inner_space = ' ' | append: section.settings.inner_space_value
    assign round = 1
  endif

  if section.settings.inner_space == false and section.settings.fluid == false
    assign rounding = ' row--rounded'
  endif

  if layout contains 'slideshow' and section.settings.fluid == false
    assign round = 1
  endif

  assign row = 'row-cols-' | append: per_row_mobile | append: ' row-cols-md-' | append: per_row | append: ' ' | append: inner_space | append: rounding


  assign modulo = section.blocks.size | modulo: per_row
  if per_row >= 2 and modulo > 0
    assign modulo_index = section.blocks.size | minus: modulo
    assign suffix = 12 | divided_by: modulo
    if per_row_mobile == 2
    endif

  endif

  if layout == 'mosaic-1'
    assign row = 'gy-divider row-cols-' | append: per_row_mobile | append: ' ' | append: inner_space | append: rounding
    assign modulo_index = blank
  endif

  if layout == 'mosaic-2'
    assign row = 'row-cols-1 row-cols-md-3' | append: ' banners-mosaic ' | append: inner_space | append: rounding
    assign modulo_index = blank
  endif

  if layout == 'mosaic-3'
    assign row = 'row-sm-column-reverse' | append: ' banners-mosaic-3 ' | append: inner_space | append: rounding
    assign modulo_index = blank
  endif

  if layout == 'mosaic-4'
    assign row = 'row-cols-1 row-cols-md-3' | append: inner_space | append: rounding
    assign modulo_index = blank
  endif

  if layout == 'mosaic-5'
    assign row = 'row-cols-1 row-cols-md-3 ' | append: inner_space | append: rounding
    assign modulo_index = blank
  endif

  assign hide_buttons_mobile = false

  if per_row_mobile == 2
    if layout == 'grid' and section.blocks.size > 1
      assign hide_buttons_mobile = true
    elsif layout == 'mosaic-1'
      assign hide_buttons_mobile = true
    endif

  endif

  if hide_buttons_mobile == true
    assign row = row | append: ' hide-buttons-mobile'
  endif

  assign show = false

  for block in section.blocks
    if block.settings.image != blank
      assign show = true
    endif
  endfor

  if section.settings.placeholder and show == false
    assign show = true
  endif
-%}

{%- if show -%}
  {% render 'section-wrapper', placement: 'top' %}

  <div class="{% render 'section-class', layout: layout %}" {% render 'section-style', layout: layout %}>
    <div class="{% render 'section-container-class', layout: layout %}">
      {% render 'section-heading', section: section %}

      {%- if layout contains 'slideshow' -%}
        {{ 'slider.css' | asset_url | stylesheet_tag }}
        {%- unless section.settings.effect == 'none' -%}{{ 'slider-effect.css' | asset_url | stylesheet_tag }}{% endunless %}
        {%- unless section.settings.boxed -%}
          <style>
            :root section#shopify-section-{{ section.id }} main-slider{--container-max-width-boxed: {{ settings.container_max_width }}px}
          </style>
        {%- endunless -%}
        {%- liquid
          assign decoration = false
          case settings.section_decoration
            when "1"
              assign section_decoration = "none"
            when "2"
              assign section_decoration = "grunge1"
            when "3"
              assign section_decoration = "grunge2"
            when "4"
              assign section_decoration = "grunge3"
            when "5"
              assign section_decoration = "paper"
            when "6"
              assign section_decoration = "mountains"
            when "7"
              assign section_decoration = "ellipse1"
            when "8"
              assign section_decoration = "ellipse2"
            when "9"
              assign section_decoration = "wave1"
            when "10"
              assign section_decoration = "wave2"
            when "11"
              assign section_decoration = "wave3"
            when "21"
              assign section_decoration = "wave4"
            when "22"
              assign section_decoration = "wave5"
            when "12"
              assign section_decoration = "triangle1"
            when "13"
              assign section_decoration = "triangle2"
            when "14"
              assign section_decoration = "triangle_assymetrical1"
            when "15"
              assign section_decoration = "triangle_assymetrical1_flip"
            when "16"
              assign section_decoration = "triangle_assymetrical2"
            when "17"
              assign section_decoration = "triangle_assymetrical2_flip"
            when "18"
              assign section_decoration = "tilt"
            when "19"
              assign section_decoration = "tilt_flip"
            when "20"
              assign section_decoration = "glaze"
          endcase
          assign section_decoration_bottom = section.settings.section_decoration_bottom

          if section_decoration_bottom != 'none'
            if section_decoration_bottom != 'global' or section_decoration != 'none'
              assign decoration = true
            endif
          endif

          assign decorCompensate = false
          assign decorCompensateSm = ''

          if decoration
            assign decorCompensate = true
            if slideshow_control_layout != 'left' and slideshow_control_layout != 'right'
              assign decorCompensateSm = 'decor-compensate-sm'
            endif
          endif
        -%}
        <main-slider {% if section.settings.slideshow_autoplay %}data-autoplay="{{ section.settings.slideshow_autoplay_speed }}000" {% if decoration %}data-bottom-decor={{ section_decoration_bottom }}{% endif %}{% if decorCompensate %} data-decor-compensate{% endif %} style="--autoplay: {{ section.settings.slideshow_autoplay_speed }}000ms"{% endif %}>
          <div style="--max-height-desktop: 65vw" class="swiper-slider-container swiper-container {% if layout contains 'modern' %}slider-boxed{% endif %} {% if layout contains 'fullheight' %}slider-fullheight {% if section.settings.slideshow_fullheight_mobile %}slider-fullheight-mobile{% endif %}{% endif %}  {% if section.settings.slider_bottom_ellipse %}slider-bottom-ellipse{% endif %}">
            <div class="swiper-wrapper">
              {%- for block in section.blocks -%}
              {%- liquid
                assign layout = 'banner'
                assign section_id = section.id
                assign product_form_id = 'product-form-' | append: section_id | append: '-' | append: block.id
                assign product_object = block.settings.url.object
                assign current_variant = product_object.selected_or_first_available_variant

                assign fetchpriority = false

                if forloop.first
                  assign fetchpriority = true
                endif

                assign font_ratio = 200 | minus: section.settings.font_ratio | divided_by: 10.0 | at_least: 2
                assign font_ratio_mobile = 200 | minus: section.settings.font_ratio_mobile | divided_by: 10 | at_least: 2

                assign slide_link = false

                if section.settings.fluid == false or block.settings.button == false
                  assign slide_link = true
                endif

                if section.settings.layout contains 'modern'
                  assign slide_link = false
                endif


                assign aligment = block.settings.aligment
                assign aligment_v = block.settings.aligment_v
                assign aligment_text = block.settings.aligment_text
                assign top_padding = block.settings.top_padding
                assign right_padding = block.settings.left_padding
                assign bottom_padding = block.settings.top_padding
                assign left_padding = block.settings.left_padding
                assign width = block.settings.width


                assign aligment_mobile = block.settings.aligment_mobile
                assign aligment_v_mobile = block.settings.aligment_v_mobile
                assign aligment_text_mobile = block.settings.aligment_text_mobile
                assign top_padding_mobile = block.settings.top_padding_mobile
                assign right_padding_mobile = block.settings.left_padding_mobile
                assign bottom_padding_mobile = block.settings.top_padding_mobile
                assign left_padding_mobile = block.settings.left_padding_mobile
                assign width_mobile = block.settings.width_mobile

                if block.settings.mobile_sync
                    assign aligment_mobile = aligment
                    assign aligment_v_mobile = aligment_v
                    assign aligment_text_mobile = aligment_text
                    assign top_padding_mobile = top_padding
                    assign right_padding_mobile = right_padding
                    assign bottom_padding_mobile = bottom_padding
                    assign left_padding_mobile = left_padding
                    assign width_mobile = width
                endif

                assign padding_desktop = section.settings.images_auto_height_padding

                if layout contains 'fullheight'
                  assign padding_mobile = false
                else
                  assign padding_mobile = section.settings.images_auto_height_padding_mobile
                endif

                assign sizes_mobile = '(min-width: 768px) 2px, (min-width: 390px) 390px, (min-width: 375px) 375px, (min-width: 320px) 320px, calc(100vw)'

                if section.settings.fluid
                  assign sizes = '(min-width: 3456px) 3456px, (min-width: 2560px) 2560px, (min-width: 1920px) 1920px, (min-width: 1536px) 1536px, (min-width: 1366px) 1366px, (min-width: 992px) 992px, (min-width: 768px) 768px,'
                  if block.settings.image_mobile == blank
                    assign sizes = sizes | append: '(min-width: 390px) 390px, (min-width: 375px) 375px, (min-width: 320px) 320px, calc(100vw)'
                  else
                    assign sizes = sizes | append: '(min-width: 2px) 2px, calc(100vw)'
                  endif
                endif

                assign ic_style = ''
                assign color = block.settings.color_placeholder
                if color != 'rgba(0,0,0,0)' and color != blank
                  assign ic_style = '--placeholder-bg: ' | append: color | append: ';'
                endif

                assign fast_shop = false
                if block.settings.fast_shop and block.settings.url.object.url contains '/products/'
                    assign fast_shop = true
                endif

                assign button_text = block.settings.button_text

                if button_text == blank
                  assign button_text = 'theme.general.shop_now' | t
                endif

                assign animation_off = false

                if forloop.first
                  assign animation_off = false
                endif

                assign class_img_only = 'fade-up'
                assign lazyload = true

                if forloop.first
                  assign class_img_only = 'lazyloaded'
                  assign lazyload = false
                endif

                unless forloop.first
                  assign after_img = '<div data-load="loading"></div>'
                endunless

                unless section.settings.lazyload
                  assign lazyload = false
                endunless

                assign custom_caption_colorization = block.settings.custom_caption_colorization

                if custom_caption_colorization
                  assign default_rgba = 'rgba(0,0,0,0)'
                  assign color_title = block.settings.color_title
                  assign color_subtitle = block.settings.color_subtitle
                  assign color_description = block.settings.color_description
                  assign color_caption_bg = block.settings.color_caption_bg

                  if color_title != default_rgba and color_title != blank
                    assign color_title = 'color:' | append: color_title | append: ';'
                  else
                    assign color_title = ''
                  endif

                  if color_subtitle != default_rgba and color_subtitle != blank
                    assign color_subtitle = 'color:' | append: color_subtitle | append: ';'
                  else
                    assign color_subtitle = ''
                  endif

                  if color_description != default_rgba and color_description != blank
                    assign color_description = 'color:' | append: color_description | append: ';'
                  else
                    assign color_description = ''
                  endif

                  if color_caption_bg != default_rgba and color_caption_bg != blank
                    assign color_caption_bg = color_caption_bg | append: ';'
                  else
                    assign color_caption_bg = false
                  endif

                else
                  assign color_title = ''
                  assign color_subtitle = ''
                  assign color_description = ''
                  assign color_caption_bg = false
                endif

              -%}

            {% if slide_link and fast_shop == false %}
              <a href="{{ block.settings.url }}"  class="swiper-slide {% if block.settings.video != null %}swiper-slide--video has-autoplay {% if slide_link and fast_shop == false %}off-control{% endif %}{% endif %}" {% if block.settings.video != null %}data-video="mp4"{% endif %}>
            {% else %}
              <div  class="swiper-slide {% if block.settings.video != null %}swiper-slide--video has-autoplay {% if slide_link and fast_shop == false %}off-control{% endif %}{% endif %}" {% if block.settings.video != null %}data-video="mp4"{% endif %}>
            {% endif %}

                {%- if section.settings.style == 'style-8' -%}
                  <div class="swiper-slide-caption-bg to-{{ aligment }} to-{{ aligment_mobile }}-mobile" {% if color_caption_bg %}style="--body-bg: {{ color_caption_bg }}"{% endif %} data-swiper-animation="animate__rotateIn" data-duration=".5s" data-delay="0s">
                    {% render 'icon-sweets-slider-decor' %}
                  </div>
              {% endif %}

                {%- if block.settings.video != null -%}
                  {%- liquid
                    assign poster = block.settings.video.preview_image | default: block.settings.image
                    assign style_video = ''
                    unless layout contains 'fullheight'
                      assign style_video = '--slider-padding-bottom-desktop:' | append: padding_desktop | append: '%; --slider-padding-bottom-mobile:' | append: padding_mobile | append: '%;'
                    endunless
                    if block.settings.video_darkness > 0
                      assign video_darkness = block.settings.video_darkness | divided_by: 100.0
                      assign style_video = style_video | append: '--video-overlay-opacity:' | append: video_darkness
                    endif
                  -%}
                  <div class="video-wrap-content w-100 js-video-wrap is-paused lazyload" style="{{ style_video }}">
                    <div class="video-control">
                      <div class="video-play" data-video-play>{% render 'icon-play' %}</div>
                      <div class="video-stop" data-video-stop>{% render 'icon-pause' %}</div>
                    </div>
                    {%- if poster != blank -%}
                      {%- liquid
                        assign ic_style = ic_style | append: '--padding-bottom: 0;--padding-bottom-mobile: 0;'
                      -%}
                      <div class="video-wrap-poster">
                        {% render 'image-container',
                          image: poster,
                          padding_backup: 41.3,
                          padding_no_bg: padding_desktop,
                          padding: padding_desktop,
                          padding_mobile: padding_mobile,
                          sizes: sizes,
                          class_ic: 'h-100  w-100'
                          round: round,
                          hover_scale: false,
                          style: ic_style,
                          class_img_only: class_img_only,
                          lazyload: lazyload,
                          fetchpriority: fetchpriority
                        %}
                      </div>
                    {%- endif -%}
                    <div data-load="loading"></div>
                    <div class="inner-video js-inner-video">
                      <template>
                        {{
                          block.settings.video
                          | video_tag:
                            image_size: '2000x',
                            autoplay: true,
                            loop: block.settings.video_loop,
                            controls: false,
                            muted: true,
                            class: "lazyload"
                        }}
                      </template>
                    </div>
                  </div>
                {%- else -%}
                  {%- if block.settings.image_mobile != blank -%}
                    {% render 'image-container',
                      image: block.settings.image_mobile,
                      padding_backup: 62.3,
                      padding_no_bg: padding_desktop,
                      padding: padding_desktop
                      padding_mobile: padding_mobile
                      sizes: sizes_mobile,
                      class_ic: 'swiper-slide-img swiper-slide-img-m',
                      round: round,
                      hover_scale: false,
                      style: ic_style,
                      class_img_only: class_img_only,
                      after_img: after_img,
                      lazyload: lazyload,
                      fetchpriority: fetchpriority
                    %}
                  {%- endif -%}

                  {%- liquid
                    if block.settings.image == blank
                      assign after_img = ''
                    endif
                  -%}
                  {% render 'image-container',
                    image: block.settings.image,
                    padding_backup: 41.3,
                    padding_no_bg: padding_desktop,
                    padding: padding_desktop,
                    padding_mobile: padding_mobile,
                    sizes: sizes,
                    class_ic: 'swiper-slide-img',
                    round: round,
                    hover_scale: false,
                    style: ic_style,
                    class_img_only: class_img_only,
                    after_img: after_img,
                    lazyload: lazyload,
                    fetchpriority: fetchpriority
                  %}
                {%- endif -%}

                <flow-type>
                  <div class="container container-fluid"
                    {%- if font_ratio != false %}data-fontratio="{{ font_ratio }}"{% endif %}
                    {% if  font_ratio_mobile != false %}data-fontratio-mobile="{{ font_ratio_mobile }}"{% endif -%}
                  >
                    <div class="swiper-slide-caption
                        to-{{ aligment }}
                        to-{{ aligment_v }}
                        to-{{ aligment_mobile }}-mobile
                        to-{{ aligment_v_mobile }}-mobile
                        text-{{ aligment_text }}
                        text-{{ aligment_text_mobile }}-mobile
                        {% if block.settings.invert_color %}colorize-invert{% endif %}
                      "
                      style="
                          --padding: {{ top_padding }}% {{ right_padding }}% {{ bottom_padding }}% {{ left_padding }}%;
                          --padding-mobile: {{ top_padding_mobile }}% {{ right_padding_mobile }}% {{ bottom_padding_mobile }}% {{ left_padding_mobile }}%;
                          --caption-width: {{ width }}%;
                          --caption-width-mobile: {{ width_mobile }}%;
                        "
                    >

                      {%- if section.settings.style == 'style-2' -%}
                        <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInLeft" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="0"{% endif %} style="--offset-top: 0; --line-height: 1em; --font-size:1em; --font-weight:{{ settings.font_weight_700 }}; {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                        <div class="swiper-slide-text heading-font {% if custom_caption_colorization == false or color_title == blank %}theme-color-strict{% endif %}" data-swiper-animation="animate__fadeInLeft" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="0.75s"{% endif %} style="--offset-top: 0; --line-height: 1em; --font-size:1em; --font-weight:{{ settings.font_weight_700 }}; {{ color_title }}">{{ block.settings.title }}</div>
                        <div class="swiper-slide-text hide-mobile" data-swiper-animation="animate__fadeInUp" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="1.4s"{% endif %} style="--offset-top: 1em; --font-size:.31em; --font-weight:{{ settings.font_weight_400 }}; --line-height: 1.4em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-3' -%}
                        {%- capture slide_1 -%}
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".4s"{% endif %} style="--offset-top: 0; --font-size: 82%;--font-weight:{{ settings.font_weight_800 }};--line-height:1.1em; {% if custom_caption_colorization and color_subtitle != blank %}{{ color_subtitle }}{% else %}color: #ffe600{% endif %}">{{ block.settings.subtitle }}</div>
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".1s"{% endif %}  style="--offset-top: 0; --font-size: 96%;--font-weight:{{ settings.font_weight_800 }};--line-height:1.1em; {% if custom_caption_colorization and color_title != blank %}{{ color_title }}{% else %}color: #ffe600{% endif %}">{{ block.settings.title }}</div>
                        {%- endcapture -%}
                        {%- capture slide_2 -%}
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".4s"{% endif %} style="--offset-top: 0; --font-size: 92%;--font-weight:{{ settings.font_weight_800 }};--line-height:1.1em; {% if custom_caption_colorization and color_subtitle != blank %}{{ color_subtitle }}{% else %}color: #ffe600{% endif %}">{{ block.settings.subtitle }}</div>
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".1s"{% endif %}  style="--offset-top: 0; --font-size: 57%;--font-weight:{{ settings.font_weight_800 }};--line-height:1.1em; {% if custom_caption_colorization and color_title != blank %}{{ color_title }}{% else %}color: #ffe600{% endif %}">{{ block.settings.title }}</div>
                        {%- endcapture -%}
                        {%- cycle slide_1, slide_2 -%}
                        <div class="swiper-slide-text  hide-mobile" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="1s"{% endif %} style="--offset-top: 2em; --font-size: 16%;--font-weight:{{ settings.font_weight_500 }};--line-height:1.8em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-4' -%}
                          <div class="swiper-slide-text" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="0"{% endif %} style="--offset-top: 0;--font-size: .15em;--font-weight: var(--font-weight-600); {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="0"{% endif %} style="--offset-top: .1em;--font-size: 57%;--font-weight: var(--font-weight-700); {{ color_title }}">{{ block.settings.title }}</div>
                          <div class="swiper-slide-text hide-mobile" data-swiper-animation="animate__fadeInUp" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".4s"{% endif %} style="--offset-top: 2em;--font-size: 15%;--font-weight: var(--font-weight-400);--line-height: 1.6em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-5' -%}
                          <div class="swiper-slide-text heading-font {% if custom_caption_colorization == false or color_subtitle == blank %}theme-color{% endif %}" data-swiper-animation="animate__fadeInDown"{% if animation_off %}data-duration="0s" data-delay="0s"{% else %} data-duration=".5s" data-delay=".5s"{% endif %} style="--offset-top: 0; --font-size: 80%;--font-weight: var(--font-weight-800);--line-height:1.1em; {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".5s" data-delay="1s"{% endif %}  style="--offset-top: 0; --font-size: 55%;--font-weight: var(--font-weight-800);--line-height:1.1em; {{ color_title }}">{{ block.settings.title }}</div>
                          <div class="swiper-slide-text hide-mobile" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".5s" data-delay="1.5s"{% endif %} style="--offset-top: 2em; --font-size: 15%;--font-weight: var(--font-weight-400);--line-height:1.8em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-6' -%}
                          <div class="swiper-slide-text heading-font {% if custom_caption_colorization == false or color_subtitle == blank %}theme-color{% endif %}" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %} data-duration=".5s" data-delay=".5s"{% endif %}  style="--offset-top: 0; --font-size: 75%;--font-weight: var(--font-weight-600);--line-height:1.1em;  {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                          <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %} data-duration=".5s" data-delay="1s"{% endif %}   style="--offset-top: 0; --font-size: 75%;--font-weight: var(--font-weight-600);--line-height:1.1em; {{ color_title }}">{{ block.settings.title }}</div>
                          <div class="swiper-slide-text hide-mobile" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %} data-duration=".5s" data-delay="1.5s"{% endif %} style="--offset-top: 2em; --font-size: 15%;--font-weight: var(--font-weight-400);--line-height:1.8em;  {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-7' -%}
                        <div class="swiper-slide-text heading-font {% if custom_caption_colorization == false or color_subtitle == blank %}theme-color{% endif %}" data-swiper-animation="animate__fadeInDown" data-duration=".5s" data-delay=".5s" style="--offset-top: 0; --font-size: 80%;--font-weight: var(--font-weight-600);--line-height:1.1em; {{ color_title }}">{{ block.settings.title }}</div>
                        <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" data-duration=".5s" data-delay="1s"  style="--offset-top: 0.1em; --font-size: 55%;--font-weight: var(--font-weight-500);--line-height:1.1em; {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                        <div class="swiper-slide-text hide-mobile" data-swiper-animation="animate__fadeIn" data-duration=".5s" data-delay="1.5s" style="--offset-top: 2em; --font-size: 20%;--font-weight: var(--font-weight-400);--line-height:1.85em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- elsif section.settings.style == 'style-8' -%}
                        <div class="swiper-slide-text heading-font {% if custom_caption_colorization == false or color_title == blank %}theme-color{% endif %}" data-swiper-animation="animate__fadeInDown" data-duration=".5s" data-delay=".5s" style="--offset-top: 0; --font-size: 75%;--font-weight: var(--font-weight-700);--line-height:1.1em; {{ color_title }}">{{ block.settings.title }}</div>
                        <div class="swiper-slide-text" data-swiper-animation="animate__fadeIn" data-duration=".5s" data-delay="1.5s" style="--offset-top: 2em; --font-size: 20%;--font-weight: var(--font-weight-400);--line-height:1.5em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- else -%}
                        <div class="swiper-slide-text" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay=".4s"{% endif %} style="--offset-top: 0;--font-size: 15%;--font-weight: {{ settings.font_weight_400 }};--line-height: 1.4em; {{ color_subtitle }}">{{ block.settings.subtitle }}</div>
                        <div class="swiper-slide-text heading-font" data-swiper-animation="animate__fadeInDown" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="0"{% endif %} style="--offset-top: .36em;--font-size: 61%;--font-weight: {{ settings.font_weight_400 }};--line-height: 1.1em; {{ color_title }}">{{ block.settings.title }}</div>
                        <div class="swiper-slide-text  hide-mobile" data-swiper-animation="animate__fadeInUp" {% if animation_off %}data-duration="0s" data-delay="0s"{% else %}data-duration=".4s" data-delay="1.4s"{% endif %} style="--offset-top: 2em;--font-size: 15%;--font-weight: {{ settings.font_weight_400 }};--line-height: 1.6em; {{ color_description }}">{{ block.settings.description }}</div>

                      {%- endif -%}

                      {%- if block.settings.button -%}
                        {%- liquid
                          if animation_off
                            assign data_delay = 'data-duration="0s" data-delay="0s"'
                          else
                            assign data_delay = 'data-duration=".4s" data-delay="1s"'
                          endif

                          assign btn_attributes = 'data-swiper-animation="animate__fadeInUp" ' | append: data_delay | append: ' style="--offset-top: 4em;"'

                          if section.settings.style == 'style-4'
                            if animation_off
                              assign data_delay = 'data-duration="0s" data-delay="0s"'
                            else
                              assign data_delay = 'data-duration=".4s" data-delay="1s"'
                            endif

                            assign btn_attributes = 'data-swiper-animation="animate__fadeInUp" data-duration=".4s" data-delay="1s" style="--offset-top: 4em;"'
                          endif
                        -%}
                        {%- if fast_shop -%}
                          {% render 'product-card-action'
                            product: product_object,
                            wrapper: false,
                            product_form_id: product_form_id,
                            show_variants: false,
                            layout: layout,
                            hide_quantity: 1,
                            w_100: 0,
                            class: block.settings.button_color,
                            btn_attributes: btn_attributes,
                            button_text: button_text,
                            current_variant: current_variant
                          %}
                        {%- else -%}

                        {% if slide_link == false %}
                        <a href="{{ block.settings.url | default: '#' }}"  class="btn {{ block.settings.button_color }}" {% if block.settings.button_target_blank %}target="_blank"{% endif %} {{ btn_attributes }}>
                      {% else %}
                        <div  class="btn {{ block.settings.button_color }}" {% if block.settings.button_target_blank %}target="_blank"{% endif %} {{ btn_attributes }}>
                      {% endif %}

                      {{ button_text }}

                      {% if slide_link == false %}
                        </a>
                      {% else %}
                        </div>

                      {%- endif -%}



                        {%- endif -%}
                      {%- endif -%}
                    </div>
                  </div>
                </flow-type>

                {% if slide_link and fast_shop == false %}
                </a>
              {% else %}
                </div>
              {% endif %}

              {%- endfor -%}
            </div>
            {%- if section.blocks.size > 1 -%}
              {%- liquid
                assign svg_width = '48'
                assign circle_cx_width = '24'
                assign circle_r_width = '23'
                assign full_side_control = false

                if slideshow_control_layout == 'left' or slideshow_control_layout == 'right'
                  assign svg_width = '40'
                  assign circle_cx_width = '20'
                  assign circle_r_width = '19'
                  assign full_side_control = true
                endif
              -%}

              {%- capture slideshow_control_layout_html_left_right -%}
                <div class="swiper-pagination-wrap {% if decorCompensate %}decor-compensate{% endif %} {% if full_side_control %}swiper-pagination--simple-{{ slideshow_control_layout }}{% endif %}">
                  <div class="swiper-pagination"></div>
                  <div class="swiper-button-prev"></div>
                  <div class="swiper-button-next">
                    {%- if section.settings.slideshow_autoplay -%}
                      <svg height="{{ svg_width }}" width="{{ svg_width }}" class="swiper-progress-bar">
                        {%- if settings.rounded_buttons_disable == "1" -%}
                          <rect width="{{ svg_width }}" height="{{ svg_width }}" stroke-width="1" fill-opacity="0"></rect>
                        {%- else -%}
                          <circle cx="{{ circle_cx_width }}" cy="{{ circle_cx_width }}" r="{{ circle_r_width }}" stroke-width="1" fill-opacity="0"></circle>
                        {%- endif -%}
                      </svg>
                    {%- endif -%}
                  </div>
                </div>
              {%- endcapture -%}

              {%- capture slideshow_control_layout_html_default -%}
                <div class="swiper-arrows-carousel container-fluid holder--max-width">
                  <div class="swiper-button-next">
                    {%- if section.settings.slideshow_autoplay -%}
                      <svg height="48" width="48" class="swiper-progress-bar">
                        {%- if settings.rounded_buttons_disable == "1" -%}
                          <rect width="42" height="42" stroke-width="1" fill-opacity="0" x="3px" y="3px" style="width:42px;height: 42px"></rect>
                        {%- else -%}
                          <circle cx="24" cy="24" r="22" stroke-width="1" fill-opacity="0"></circle>
                        {%- endif -%}
                      </svg>
                    {%- endif -%}
                  </div>
                  <div class="swiper-button-prev"></div>
                </div>
              {%- endcapture -%}

              {%- if slideshow_control_layout == 'none' -%}
                <div class="swiper-pagination {% if decorCompensate %}decor-compensate{% endif %}"></div>
              {%- elsif slideshow_control_layout == 'default' -%}
                <div class="swiper-pagination {% if decorCompensate %}decor-compensate{% endif %}"></div>
                {{ slideshow_control_layout_html_default }}
              {%- else -%}
                {{ slideshow_control_layout_html_left_right }}
              {%- endif -%}


            {%- endif -%}
          </div>
          {%- unless section.settings.effect == 'none' -%}<slider-effect data-effect="{{ section.settings.effect }}"><canvas></canvas></slider-effect>{% endunless %}
        </main-slider>
      {%- else -%}
        <div class="row {{ row }}">
          {%- for block in section.blocks -%}
            {%- liquid

              assign  inner_space_value = section.settings.inner_space_value
              capture image_default_cycle
                cycle 'pets_banner_1.png', 'pets_banner_2.png', 'pets_banner_3.png', 'pets_banner_4.png'
              endcapture

              if layout == 'mosaic-1'

                if inner_space_value == 'g-1'
                  assign padding_bottom_1 = '71.0'
                elsif inner_space_value == 'g-2'
                  assign padding_bottom_1 = '71.0'
                else
                  assign padding_bottom_1 = '71.9'
                endif

                capture font_ratio_cycle
                  cycle '7.93', '11.1', '11.1', '7.93'
                endcapture

                capture padding_cycle
                  if section.settings.inner_space
                    cycle padding_bottom_1, '50.4', '50.4', padding_bottom_1
                  else
                    cycle '71.9', '51.36', '51.36', '71.9'
                  endif
                endcapture

              elsif layout == 'mosaic-2'

                if inner_space_value == 'g-1'
                  assign padding_bottom_1 = '61'
                elsif inner_space_value == 'g-2'
                  assign padding_bottom_1 = '60.33'
                else
                  assign padding_bottom_1 = '57.33'
                endif

                capture padding_cycle
                  if section.settings.inner_space
                    cycle '123.16', padding_bottom_1, padding_bottom_1, '123.16'
                  else
                    cycle '122.16', '61.1', '61.1', '122.16'
                  endif
                endcapture

                capture style_cycle
                  cycle '', 'font-size:.39em;', 'font-size:.39em;', ''
                endcapture

              elsif layout == 'mosaic-3'

                if inner_space_value == 'g-1'
                  assign padding_bottom_1 = '80.5'
                elsif inner_space_value == 'g-2'
                  assign padding_bottom_1 = '79.6'
                else
                  assign padding_bottom_1 = '76.5'
                endif

                capture font_ratio_cycle
                  cycle '4.2', '6.2', '4.2'
                endcapture

                capture padding_cycle
                  if section.settings.inner_space
                    cycle '162', padding_bottom_1, '162'
                  else
                    cycle '162', '81', '162'
                  endif
                endcapture

              elsif layout == 'mosaic-4'
                capture font_ratio_cycle
                  cycle '7.6', '5.6', '5.6', '7.6'
                endcapture

                capture padding_cycle
                    cycle '49.8', '109', '109', '49.8'
                endcapture

                capture style_cycle
                  cycle '', 'font-size:.39em;', 'font-size:.39em;', ''
                endcapture

              elsif layout == 'mosaic-5'

                if inner_space_value == 'g-1'
                  assign padding_bottom_1 = '58.3'
                  assign padding_bottom_2 = '118'
                elsif inner_space_value == 'g-2'
                  assign padding_bottom_1 = '59'
                  assign padding_bottom_2 = '121'
                else
                  assign padding_bottom_1 = '57.3'
                  assign padding_bottom_2 = '123'
                endif

                capture padding_cycle
                  if section.settings.inner_space
                    cycle padding_bottom_2, padding_bottom_1, padding_bottom_1,  padding_bottom_1, padding_bottom_1, padding_bottom_1, padding_bottom_2
                  else
                    cycle '116.5', '58.3', '58.3', '58.3', '58.3', '58.3', '116.5'
                  endif
                endcapture

              endif

              assign start = false
              assign end = false

              if layout == 'mosaic-2'
                assign cycle_count = 4
                assign shift = 2

                if forloop.index == shift
                  assign start = true

                else
                  assign modulo = forloop.index | divided_by: cycle_count | times: cycle_count | plus: shift
                  assign modulo = forloop.index | modulo: modulo

                  if modulo == 0
                    assign start = true
                  endif
                endif

                assign shift = 3

                if forloop.index == shift
                  assign end = true

                else
                  assign modulo = forloop.index | divided_by: cycle_count | times: cycle_count | plus: shift
                  assign modulo = forloop.index | modulo: modulo

                  if modulo == 0
                    assign end = true
                  endif
                endif

              elsif layout == 'mosaic-4'
                assign modulo = forloop.index | modulo: 2

                if modulo == 0
                  assign end = true
                else
                  assign start = true
                endif

              elsif layout == 'mosaic-5'
                assign modulo = forloop.index | modulo: 7

                if modulo == 1 or modulo == 3 or modulo == 6
                  assign start = true

                elsif modulo == 2 or modulo == 5 or modulo == 0
                  assign end = true
                endif

              endif
            -%}
            {%- if start -%}
              <div class="col">
                <div class="row row-cols-1 {% if section.settings.inner_space %}{{ inner_space }}{% else %}g-0{% endif %}">
            {%- endif -%}

            <div class="
              {%- liquid
                if layout contains 'mosaic-1'
                  if per_row_mobile == 2
                    echo 'col '
                    if forloop.last
                      assign modulo_mobile = section.blocks.size | modulo: per_row_mobile
                      if modulo_mobile != 0
                        echo 'col-12 '
                      endif
                    endif
                  endif
                  cycle 'col-md-5', 'col-md-7', 'col-md-7', 'col-md-5'

                else
                  echo 'col '
                  if per_row_mobile == 2 and forloop.last
                    assign modulo_mobile = section.blocks.size | modulo: per_row_mobile
                    if modulo_mobile != 0
                      echo 'col-12-before-md'
                    endif
                  endif
                  if modulo_index != blank and modulo_index < forloop.index
                    echo ' col-md-' | append: suffix
                  endif

                endif
              -%}
            " {{ block.shopify_attributes }}>

              {%- if block.settings.style != 'style-4' -%}
                <flow-type>
              {%- endif -%}
              {% render 'banner',
                section: section,
                block: block,
                sizes: sizes,
                round: round,
                style: section.settings.style,
                font_ratio_cycle: font_ratio_cycle,
                padding_cycle: padding_cycle,
                style_cycle: style_cycle,
                image_default_cycle: image_default_cycle,
                modulo: modulo,
                last: forloop.last,
                button_text: button_text
              %}

              {%- if block.settings.style != 'style-4' -%}
                </flow-type>
              {%- endif -%}
            </div>

            {%- if end -%}
                </div>
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>
      {%- endif -%}

    </div>
  </div>
  {% render 'section-wrapper', decorCompensateSm: decorCompensateSm %}
{%- endif -%}

{% schema %}
{
  "name": "Banners grid",
